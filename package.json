{
  "name": "client-edi",
  "productName": "ClientEDI",
  "version": "1.0.0",
  "description": "Client Edi Application Electron TIVIT",
  "homepage": "https://github.com/maximegris/angular-electron",
  "author": {
    "name": "Maxime GRIS",
    "email": "maxime.gris@gmail.com"
